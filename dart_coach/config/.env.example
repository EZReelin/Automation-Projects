# Dart Performance Analysis System - Environment Variables
# Copy this file to .env and fill in your credentials

# Scolia Web Client Credentials
SCOLIA_USERNAME=your_scolia_username
SCOLIA_PASSWORD=your_scolia_password

# Dart Connect Credentials
DART_CONNECT_USERNAME=your_dartconnect_username
DART_CONNECT_PASSWORD=your_dartconnect_password

# GoDartsPro Credentials
GODARTSPRO_USERNAME=your_godartspro_username
GODARTSPRO_PASSWORD=your_godartspro_password

# Google Calendar Integration
GOOGLE_CALENDAR_CREDENTIALS_FILE=/path/to/credentials.json
GOOGLE_CALENDAR_TOKEN_FILE=/path/to/token.json

# Ollama Configuration
OLLAMA_HOST=http://localhost:11434

# Camera Device ID (for OBSBOT Tiny Lite 2)
CAMERA_DEVICE_ID=0

# Audio Device ID (for voice observations)
AUDIO_DEVICE_ID=default
